function diagonalDifference(arr) {
    var diagOne = 0;
    for (var i= 0, j=0; i < arr.length, j < arr.length; i++, j++) {
        diagOne += arr[i][j];
    }
    var diagTwo = 0;
    for (var i= 0, j=arr.length-1; i < arr.length, j >=0; i++, j--) {
        diagTwo += arr[i][j];
    }
    return Math.abs(diagOne - diagTwo);  
    
}
